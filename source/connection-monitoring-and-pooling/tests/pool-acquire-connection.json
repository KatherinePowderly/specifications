{
  "version": 1,
  "style": "unit",
  "description": "must be able to acquire a connection",
  "operations": [
    {
      "name": "acquire"
    }
  ],
  "events": [
    {
      "type": "connectionAcquisitionStarted",
      "pool": {
        "id": 1
      }
    },
    {
      "type": "connectionAcquired",
      "pool": {
        "id": 1
      },
      "connection": {
        "id": 1
      }
    }
  ],
  "ignore": [
    "connectionCreated",
    "connectionPoolCreated"
  ]
}
