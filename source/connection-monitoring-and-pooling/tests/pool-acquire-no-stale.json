        "id": 1,
        "generation": 0
      }
    },
    {
      "type": "connectionCreated",
      "connection": {
        "id": 2,
        "generation": 0
      }
    },
    {
      "type": "connectionCreated",
      "connection": {
        "id": 3,
        "generation": 0
      }
    },
    {
      "type": "connectionPoolCleared",
