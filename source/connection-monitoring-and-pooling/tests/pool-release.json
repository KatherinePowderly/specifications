{
  "version": 1,
  "style": "unit",
  "description": "must have a method of allowing the driver to release a connection",
  "operations": [
    {
      "name": "acquire",
      "label": "conn"
    },
    {
      "name": "release",
      "connection": "conn"
    }
  ],
  "events": [
    {
      "type": "connectionReleased",
