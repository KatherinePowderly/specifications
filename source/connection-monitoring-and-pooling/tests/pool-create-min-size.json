{
  "version": 1,
  "style": "unit",
  "description": "must be able to start a pool with minPoolSize connections",
  "poolOptions": {
    "minPoolSize": 5
  },
  "operations": [
    {
      "name": "wait",
      "ms": 50
    },
    {
      "name": "checkOut"
    }
  ],
  "events": [
    {
      "type": "ConnectionPoolCreated"
    },
    {
      "type": "ConnectionCreated"
    },
    {
      "type": "ConnectionCreated"
    },
    {
      "type": "ConnectionCreated"
    },
    {
      "type": "ConnectionCreated"
    },
    {
      "type": "ConnectionCreated"
    },
    {
      "type": "ConnectionCheckOutStarted"
    },
    {
      "type": "ConnectionCheckedOut"
    }
  ],
  "ignore": [
    "ConnectionReady",
    "ConnectionClosed"
  ]
}
